{
    "name": "Image Injector",
    "description": "Inject image into Google homepage to replace logo",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "16": "./icons/icon-16.png",
            "32": "./icons/icon-32.png",
            "48": "./icons/icon-48.png",
            "128": "./icons/icon-128.png"
        },
        "default_title": "Popup",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}