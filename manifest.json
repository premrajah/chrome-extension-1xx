{
    "name": "Image Injector",
    "description": "Inject image into Google homepage to replace logo",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "./icons/icon-16.png",
        "32": "./icons/icon-32.png",
        "48": "./icons/icon-48.png",
        "128": "./icons/icon-128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}